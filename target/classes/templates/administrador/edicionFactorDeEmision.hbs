{{#partial "contenido" }}
<div class="row my-5">
    <div class="col">
        <section class="container-fluid p-5 bg-light" id="def">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-xl-6 col-md-6 col-sm-12 col-12">
                        <h2 class="pb-3 text-center mb-5">Editar factor de emision</h2>
                            <form class="needs-validation" method="POST" action="/administrador/{{usuario.id}}/factoresDeEmision/{{factorDeEmision.id}}" novalidate>
                                <div class="form-row">
                                <div class="col-md-3 mb-3">
                                  <label for="validationCustom03">Factor de emision</label>
                                  <input type="text" class="form-control" id="validationCustom03" placeholder="{{factorDeEmision.id}}" disabled name="factorDeEmision">
                                </div>
                                <div class="form-row">
                                    <button class="btn btn-primary" type="submit" >Guardar y volver </button>
                                </div>
                            </form>
                    </div>
                </div>
            </div>
        </section>

    </div>
</div>
{{/partial}}
{{> administrador/baseAdministrador.hbs }}