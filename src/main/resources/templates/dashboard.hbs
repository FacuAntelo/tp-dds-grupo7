{{#partial "contenido" }}
                <div class="row my-5">
                    <h3 class="fs-4 mb-3">Huella de carbono por sector </h3>
                    <div class="col">
                        <table class="table bg-white rounded shadow-sm  table-hover">
                            <thead>
                                <tr>
                                    <th scope="col" width="50">#</th>
                                    <th scope="col">Sector</th>
                                    <th scope="col">Huella de carbono</th>
                                </tr>
                            </thead>
                            <tbody>
                            {{#each reportes}}
                                <tr>
                                    <th scope="row"></th>
                                    <td>{{sector}}</td>
                                    <td>{{valorHCconUnidad}}</td>
                                </tr>
                            {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
{{/partial}}
{{> baseOrganizacion.hbs }}
